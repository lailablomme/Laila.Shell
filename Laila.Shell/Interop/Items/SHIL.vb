Public Enum SHIL As Integer
    SHIL_LARGE = 0        ' Large icons (32x32)
    SHIL_SMALL = 1        ' Small icons (16x16)
    SHIL_EXTRALARGE = 2   ' Extra large icons (48x48)
    SHIL_SYSSMALL = 3     ' System small icons (compressed 16x16)
    SHIL_JUMBO = 4        ' Jumbo icons (256x256)
End Enum